<html lang="en">

<head>
    <!-- <link href="https://vjs.zencdn.net/7.0.5/video-js.css" rel="stylesheet"> -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
    <link href="https://vjs.zencdn.net/7.0.5/video-js.css" rel="stylesheet">
</head>
<style>
    .video-js {
        display: block;
        vertical-align: top;
        box-sizing: border-box;
        color: #fff;
        background-color: #000;
        padding: 0;
        font-size: 10px;
        line-height: 1;
        font-weight: normal;
        font-style: normal;
        font-family: Arial, Helvetica, sans-serif;
        word-break: initial;
        position: unset;
    }
</style>

<body id="body" style="background-color: rgb(0, 0, 0);display: block;">
    <script>
        let src = "<%= src %>";
        if(src.includes('drive.google.com') || src.includes('openload')){
            $('#body').append(
                '<iframe width="100%" height="97%" style="border: none" src="<%= src %>" allowfullscreen="true" webkitallowfullscreen="true"></iframe>'
            );
        } else if(src !== 'error'){
            $('#body').append('<video id="my-video" class="video-js" style="width:100%" controls preload="auto" data-setup="{}"><source src="' +src + '"type="video/mp4"></video>');
       
        } else {
            $('body').append('<img src="https://drudesk.com/sites/default/files/404-error-page-not-found.jpg"'>);
        }
    </script>
    <script src="https://vjs.zencdn.net/7.0.5/video.js"></script>


</body>

</html>
